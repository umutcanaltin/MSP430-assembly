;-------------------------------------------------------------------------------
; MSP430 Assembler Code Template for use with TI Code Composer Studio
;
;
;-------------------------------------------------------------------------------
            .cdecls C,LIST,"msp430.h"       ; Include device header file
            
;-------------------------------------------------------------------------------
            .def    RESET
             ; Export program entry-point to
            .data
			.global movlist

list1 		.word 9712, 2454, 1275, 3312, 4243, 2395, 4387, 12236, 9454, 32221
list2 		.word 0x10                                        ; make it known to linker.
;-------------------------------------------------------------------------------
            .text                           ; Assemble into program memory.
            .retain                         ; Override ELF conditional linking
                                            ; and retain current section.
            .retainrefs                     ; And retain any sections that have
                                            ; references to current section.

;-------------------------------------------------------------------------------
RESET       mov.w   #__STACK_END,SP         ; Initialize stackpointer
StopWDT     mov.w   #WDTPW|WDTHOLD,&WDTCTL  ; Stop watchdog timer


;-------------------------------------------------------------------------------
; Main loop here

movlist		mov.w 	#list1,r15
		mov.w 	#10000,r14
		mov.w	#0,r9
		mov.w 	#list2,r8
loop 		mov.w @r15,r13 ; M(r15) â€¡ r13
		add.w #2,r15 ; r15 += 2 (move to the next element)
		cmp.w r15,r8
		jn	$
		cmp.w r14,r13
		jn loop
		add.w #1,r9
		jmp loop



;-------------------------------------------------------------------------------

                                            

;-------------------------------------------------------------------------------
; Stack Pointer definition
;-------------------------------------------------------------------------------
            .global __STACK_END
            .sect   .stack
            
;-------------------------------------------------------------------------------
; Interrupt Vectors
;-------------------------------------------------------------------------------
            .sect   ".reset"                ; MSP430 RESET Vector
            .short  RESET
            
